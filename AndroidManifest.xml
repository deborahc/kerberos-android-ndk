<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="edu.mit.kerberos"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10" />
    
    <permission android:description="@string/app_name"
        android:label="Talk to Dummy Kerberos"
        android:name="com.example.dummyKerb.SEND_PERM"
        android:protectionLevel="dangerous"></permission>
    <permission android:description="@string/app_name"
        android:label="Talk to Dummy Kerberos"
        android:name="com.example.dummyKerb.LISTEN_PERM"
        android:protectionLevel="dangerous"></permission>
    
    <uses-permission android:name="com.example.dummyKerb.SEND_PERM" />
    <uses-permission android:name="com.example.dummyKerb.LISTEN_PERM" />
	<uses-permission android:name="android.permission.INTERNET" /> 
	
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:name="KerberosAppActivity"
                  android:label="@string/app_name"
                  android:theme="@android:style/Theme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest> 
